database: powernetworks2, mysql

create table company(cin varchar(30) primary key, name varchar(1000), incorp_date varchar(20), regnum varchar(15), type varchar(100), listing varchar(100), busscategory varchar(150), companynature varchar(150), subcategory varchar(150), regauth varchar(100), authcap varchar(100), paidupcap varchar(100), email varchar(150), address varchar(2000), status varchar(150));

cin;name;startdate;regnum;typeofcompany;listingtype;busscategory;companynature;subcategory;regauth;authcap;paidupcap;email;comp.address;status

create table businessperson(din varchar(50) primary key, name varchar(500), address varchar(2000));

create table worksin(cin varchar(30) references company(cin),din varchar(50) references businessperson(din), designation varchar(50), primary key (cin,din));

LOAD DATA LOCAL INFILE '/home/abhishek/Desktop/project/power_networks-master/djnagowork/tempsite/src/unsure/MCA_DB/companiesall.csv' IGNORE
INTO TABLE company
FIELDS TERMINATED BY ';'
LINES TERMINATED BY '\n'
IGNORE 1 ROWS;

